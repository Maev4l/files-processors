{
  "name": "converter",
  "version": "1.0.0",
  "scripts": {
    "serve": "webpack serve --config ./client/webpack.config.dev.babel.js --progress --color",
    "compile": "webpack --config ./client/webpack.config.prod.babel.js --progress",
    "tf-init": "cd ./infra && terraform init",
    "tf-apply": "cd ./infra && terraform apply -auto-approve && terraform output -json > ../infra.json && aws s3 cp ../infra.json s3://global-tf-states/pdf-converter/infra.json",
    "tf-output": "cd ./infra && terraform output -json",
    "download-infra": "aws s3 cp s3://global-tf-states/pdf-converter/infra.json ./infra.json ",
    "deploy-frontend": "yarn compile && node ./client/deploy"
  },
  "dependencies": {
